{
  "manifest_version": 3,
  "name": "Website Checker",
  "version": "1.0",
  "description": "Check website information (WHOIS, DNS, SSL, etc.)",
  "permissions": ["activeTab", "scripting", "storage", "tabs"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
